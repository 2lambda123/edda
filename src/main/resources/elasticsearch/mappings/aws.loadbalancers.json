                "index": "not_analyzed"
            },
            "ltime": {
                "type": "date",
                "format": "dateOptionalTime"
            },
            "mtime": {
                "type": "date",
                "format": "dateOptionalTime"
            },
            "stime": {
                "type": "date",
                "format": "dateOptionalTime"
            },
            "tags": {
                "type": "object",
