                    },
                    "class": {
                        "type": "string",
                        "index": "not_analyzed"
                    },
                    "createdTime": {
                        "type": "date",
                        "format": "dateOptionalTime"
                    },
                    "healthCheck": {
                        "dynamic": "true",
                        "properties": {
                            "class": {
                                "type": "string",
                                "index": "not_analyzed"
